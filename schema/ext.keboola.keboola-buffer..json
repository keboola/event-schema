{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "string"
    },
    "params": {
      "type": "object",
      "properties": {
        "task": {
          "type": "string"
        }
      },
      "required": [
        "task"
      ]
    },
    "results": {
      "type": "object",
      "properties": {
        "projectId": {
          "type": "integer"
        },
        "error": {
          "type": "string"
        }
      },
      "required": [
        "projectId"
      ]
    }
  },
  "required": [
    "message",
    "params",
    "results"
  ]
}
