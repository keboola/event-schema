{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "number"
    },
    "event": {
      "type": "string"
    },
    "created": {
      "type": "string"
    },
    "message": {
      "type": "string"
    },
    "params": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "version": {
          "type": "number"
        }
      },
      "required": ["name", "version"]
    }
  },
  "required": ["id", "event", "created", "message", "params"]
}
